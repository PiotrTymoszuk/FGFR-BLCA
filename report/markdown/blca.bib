keywords = {65Y20,Computational efficiency,Divide and conquer,Gower's interpolation formula,Landmark MDS,Pivot MDS,Procrustes transformation MSC Classification: 62H9},
month = {jul},
title = {{Multidimensional Scaling for Big Data}},
