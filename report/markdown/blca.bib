journal = {Frontiers in Immunology},
keywords = {CD8+ T cells,Immune exclusion,Immunotherapy,T cell-inflammation,Tumor microenvironment,WNT/$\beta$-catenin signaling pathway},
month = {sep},
number = {SEP},
pages = {2293},
pmid = {31616443},
