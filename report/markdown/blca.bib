author = {Helal, Duaa S. and Darwish, Sara A. and Awad, Radwa A. and Ali, Dina A. and El-Guindy, Dina M.},
doi = {10.1186/S13000-023-01295-Y/FIGURES/7},
file = {:C\:/Users/piotr/AppData/Local/Mendeley Ltd./Mendeley Desktop/Downloaded/Helal et al. - 2023 - Immunohistochemical based molecular subtypes of muscle-invasive bladder cancer association with HER2 and EGFR alte.pdf:pdf},
