journal = {Diagnostic Pathology},
keywords = {Basal,GATA3,CK5/6,p53,HER2,EGFR,Luminal,Molecular classification,Muscle invasive bladder cancer,Response to chemotherapy,Survival},
month = {dec},
number = {1},
pages = {1--13},
